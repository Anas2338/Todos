{
  "buildCommand": "cd fullstack-todo/frontend && npm run build",
  "outputDirectory": "fullstack-todo/frontend/.next",
  "installCommand": "cd fullstack-todo/frontend && npm install",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/fullstack-todo/frontend/$1"
    }
  ]
}
